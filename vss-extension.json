{
    "manifestVersion": 1,
    "id": "apptestai-test",
    "name": "apptest.ai test",
    "version": "0.2.2",
    "publisher": "apptestai",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "vsts extension for apptest.ai test execution.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "icon.png"        
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "files": [
        {
            "path": "tasks/apptest.ai"
        },
        {
            "path": "src/apptest.ai/node_modules",
            "packagePath": "tasks/apptest.ai/node_modules"
        },
        {
            "path": "src/apptest.ai/task.json",
            "packagePath": "tasks/apptest.ai/task.json"
        },
        {
            "path": "src/apptest.ai/package.json",
            "packagePath": "tasks/apptest.ai/package.json"
        }
    ],
    "tags": [
        "mobile test",
        "ios",
        "android",
        "test task",
        "testing"
    ],
    "contributions": [
        {
            "id": "apptestai-test",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/apptest.ai"
            }
        }
    ]
}
